{
  "name": "skill-share-api-gateway",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "proto:build": "yarn proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=src/protos/ src/protos/*.proto",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsoa:gen": "tsoa spec-and-routes -c ./tsoa.json",
    "start-production": "npm run build:with-swagger && node dist/main.js",
    "build:with-swagger": "npm run format && tsoa spec-and-routes && npm exec tsc",
    "build:without-swagger": "npm run format && tsoa routes && npm exec tsc",
    "dev": "nodemon --watch src --ignore ./src/routes.ts --ignore ./src/swagger.json -e ts --exec \"npm run tsoa:gen && npm exec tsc && node ./dist/main.js\" --delay 2",
    "postinstall": "tsc",
    "start": "node ./dist/main.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.0",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.13",
    "tsoa": "^3.12.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.1",
    "@grpc/proto-loader": "^0.6.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.0",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^0.23.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemon": "^2.0.13",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.12.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "15.8.0"
  }
}
